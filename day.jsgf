#JSGF V1.0;

grammar day;

public <day> = [el] [dia] <dayNumber>;

<dayNumber> = uno | dos | tres | cuatro | cinco | seis | siete | ocho |
    nueve | diez | once | doce | trece | catorce | quince | dieciseis | diecisiete |
    dieciocho | diecinueve | veinte | veintiuno | veintidos | veintitres | veinticuatro |
    veinticinco | veintiseis | veintisiete | veintiocho | veintinueve | treinta | 
    treinta y uno;